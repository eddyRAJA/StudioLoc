{% extends 'base.html.twig' %}

{% block title %}Studio{% endblock %}

{% block body %}

  <div class="container">
    <h1 class="mt-0">
      {{ studio.name }}
      studios</h1>

    <img class="align-self-start mr-3" height="300" src="{{ studio.poster }}" alt="poster of {{ studio.name }} ">
    <p>Summary :
      {{ studio.summary }}</p>
    <p>detail :
      {{ studio.detail }}</p>
    <p>unitPrice :
      {{ studio.unitPrice }}</p>
    {# 
      <p>Created on:
        {{ company.createdAt }}</p>
      <p>Updated on:
        {{ company.updatedAt }}</p>#}
    <div>
      <p>
        <a href="" class="btn btn-lg btn-primary">I rent</a>
      </p>
    </div>
    <p>slug :
      {{ studio.slug }}</p>

  </div>

</div>
<div class="container">
  <pre>
    {{ dump(studio) }}
</pre>
</div>

{% endblock %}
